# int func(int a, int b, int c) {
#     int d = 0;
#     for (int i = 0; i < a; i++) {
#         if (d < b)
#             d += c;
#     }
#     return d;
# }

# TEST_2

# int factorial(int n) {
#     if (n <= 1) return 1;
#     return n * factorial(n - 1);
# }

# TEST_3

# int loop(int a, int b, int n) {
#     int res = 0;
#     if (a < b) {
#         for (int i = 0; i < n; ++i) {
#             res *= 2;       
#         }
#     }
#     else {
#         while (a >= b) {
#             b--;
#             res += a - b;
#         }
#     }
#     return res;
# }

